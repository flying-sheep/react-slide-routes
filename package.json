{
  "name": "react-slide-routes",
  "version": "2.0.0",
  "description": "The easiest way to slide React routes",
  "keywords": [
    "react",
    "slide",
    "router",
    "routes",
    "route",
    "transition",
    "animation",
    "motion"
  ],
  "main": "dist/react-slide-routes.cjs.js",
  "module": "dist/react-slide-routes.esm.js",
  "typings": "dist/src/index.d.ts",
  "repository": "https://github.com/nanxiaobei/react-slide-routes.git",
  "author": "nanxiaobei <nanxiaobei@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "concurrently npm:watch npm:serve",
    "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "yarn build -w",
    "serve": "serve -s"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router": ">=6.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.5",
    "concurrently": "^7.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0-next.91",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "rollup": "^3.3.0",
    "serve": "^14.1.2",
    "typescript": "^4.9.3"
  }
}
